    <GORRSQL>
        <GORRSQL_SQPR_CODE>SSB_ROLES</GORRSQL_SQPR_CODE>
        <GORRSQL_SQRU_CODE>SSB_ROLE_PROXYMGMT</GORRSQL_SQRU_CODE>
        <GORRSQL_SEQ_NO>2</GORRSQL_SEQ_NO>
        <GORRSQL_ACTIVE_IND>Y</GORRSQL_ACTIVE_IND>
        <GORRSQL_VALIDATED_IND>Y</GORRSQL_VALIDATED_IND>
        <GORRSQL_PARSED_SQL>-- *** This is a sample template ***
-- This template may be used to formulate custom logic to determine which SSB role(s) a person should be in.
-- The select statement must be of the form:
--     SELECT CAST(''TRUE'' AS VARCHAR2(4)) {specific SSB role in UPPERCASE}
-- where {specific SSB role in UPPERCASE} is the name of a role as defined in TWTVROLE
-------------------------------------------------------------------------------------------------------------
SELECT CAST(''TRUE'' AS VARCHAR2(4)) PROXYMGMT
FROM TWGRROLE
WHERE TWGRROLE_ROLE = ''WTAILORPROXYMGMT''
AND TWGRROLE_PIDM = :PIDM
        </GORRSQL_PARSED_SQL>
    </GORRSQL>
